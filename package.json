{
  "type": "module",
  "name": "kickoff-sveltekit",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/migrate",
    "db:reset": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/reset",
    "db:studio": "drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "pnpm db:test:reset && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "bits-ui": "^0.15.1",
    "clsx": "^2.1.0",
    "cssnano": "^6.0.3",
    "dotenv": "^16.4.1",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "formsnap": "^0.4.3",
    "postcss": "^8.4.33",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "radix-icons-svelte": "^1.2.1",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "sveltekit-superforms": "^1.13.4",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-node": "^1.2.2",
    "vitest": "^1.2.2",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.501.0",
    "@aws-sdk/s3-request-presigner": "^3.501.0",
    "@lucia-auth/adapter-drizzle": "^1.0.0",
    "@lucia-auth/adapter-postgresql": "^3.0.0",
    "@sendgrid/mail": "^8.1.0",
    "drizzle-orm": "^0.29.3",
    "lucia": "^3.0.1",
    "oslo": "^1.0.2",
    "pg": "^8.11.3",
    "svelte-email": "^0.0.4",
    "svelte-sonner": "^0.3.11",
    "sveltekit-flash-message": "^2.4.1"
  }
}
