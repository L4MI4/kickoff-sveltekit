{
  "type": "module",
  "name": "kickoff-sveltekit",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "vite-node --options.transformMode.ssr='/.*/' src/lib/db/migrate",
    "db:test:migrate": "NODE_ENV=test vite-node --options.transformMode.ssr='/.*/' src/lib/db/migrate",
    "db:test:prepare": "NODE_ENV=test vite-node --options.transformMode.ssr='/.*/' src/lib/db/reset",
    "db:studio": "drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte . --check . && eslint .",
    "format": "prettier --write  --plugin prettier-plugin-svelte .",
    "test:integration": "pnpm db:test:prepare && NODE_ENV=test playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "bits-ui": "^0.11.8",
    "clsx": "^2.0.0",
    "cssnano": "^6.0.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "formsnap": "^0.4.1",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "radix-icons-svelte": "^1.2.1",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-french-toast": "^1.2.0",
    "sveltekit-superforms": "^1.12.0",
    "tailwind-merge": "^2.1.0",
    "tailwind-variants": "^0.1.19",
    "tailwindcss": "^3.3.7",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-node": "^1.0.4",
    "vitest": "^1.0.4",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.476.0",
    "@aws-sdk/s3-request-presigner": "^3.476.0",
    "@lucia-auth/adapter-postgresql": "^2.0.2",
    "@sendgrid/mail": "^8.1.0",
    "drizzle-orm": "^0.29.1",
    "lucia": "^2.7.5",
    "pg": "^8.11.3",
    "svelte-email": "^0.0.4",
    "sveltekit-flash-message": "^2.3.0"
  }
}
